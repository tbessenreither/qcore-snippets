{
	"name": "qcore-snippets",
	"displayName": "qcore-snippets",
	"description": "This package contains the snippets for the qcore template engine.",
	"version": "1.1.1",
	"publisher": "tbessenreither",
	"engines": {
		"vscode": "^1.66.0"
	},
	"scripts": {
		"package": "vsce package"
	},
	"categories": [
		"Snippets"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/tbessenreither/qcore-snippets"
	},
	"contributes": {
		"grammars": [
			{
				"injectTo": [
					"text.html.derivative",
					"text.html.php",
					"source.css"
				],
				"scopeName": "html.qcore",
				"path": "./syntaxes/qcore.tmGrammar.json"
			}
		],
		"snippets": [
			{
				"language": "html",
				"path": "./snippets/snippets.code-snippets"
			},
			{
				"language": "php",
				"path": "./snippets/snippets.code-snippets"
			}
		]
	}
}
