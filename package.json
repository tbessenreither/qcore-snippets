{
	"name": "qcore-snippets",
	"displayName": "qcore-snippets",
	"description": "This package contains the snippets for the qcore template engine.",
	"version": "1.0.2",
	"publisher": "tbessenreither",
	"engines": {
		"vscode": "^1.66.0"
	},
	"categories": [
		"Snippets"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/tbessenreither/qcore-snippets"
	},
	"contributes": {
		"languages": [
			{
				"id": "qcore",
				"extensions": [
					".html", ".htm", ".php"
				]
			}
		],
		"grammars": [
			{
				"language": "html",
				"scopeName": "text.html.derivative",
				"path": "./syntaxes/qcore.tmGrammar.json"
			}
		],
		"snippets": [
			{
				"language": "html",
				"path": "./snippets/snippets.code-snippets"
			},
			{
				"language": "php",
				"path": "./snippets/snippets.code-snippets"
			}
		]
	}
}